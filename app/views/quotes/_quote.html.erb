<div class="q">
	<span class="num" style="display:none;">1</span>
	<blockquote><p><%= quote.text %></p></blockquote>
	<span class="author"><%= quote.author.name %></span>
	<ul class="tags" style="display:none;">
    <% quote.tags.each do |tag| %>
      <li><a href="#"><%= tag.name %></a></li>
    <% end %>
  </ul>
</div>
